{
  "builds": [
    {
      "src": "youtubesummarizer/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "youtubesummarizer/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles_build"
}
